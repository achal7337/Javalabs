package practice;
import java.util.Arrays;
import java.util.Objects;
import java.util.Scanner;

public class practice {
	
	
	private double  a = 1.65;
	private double  b = 10.23;

public practice() {
	this.a=0.0;
	this.b=0.0;
}

public double sum() {
	return a+b;
}

	
public static void main(String[] args) {
	practice a =  new practice();
	a.a=10.2;
	System.out.print(a.a+" "+ a.b+" " +a.sum());

	

	 }
	

}
