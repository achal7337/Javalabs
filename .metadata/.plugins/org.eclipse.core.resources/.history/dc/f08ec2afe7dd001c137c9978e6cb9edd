package practice;
import java.util.Scanner;

public class practice {
	
	
	public static void main(String[] args) {
		int[] anArray;
		anArray = new int[10];
		int pos =0;
		int neg=0;
		int odd=0;
		int even=0;
		int zero=0;
		for(int i=0;i<anArray.length;i++) {
		Scanner myObj = new Scanner(System.in);
		System.out.println("enter number:");
		
		int no = myObj.nextInt();
		anArray[i]=no;
		
		if (anArray[i]>0) pos++;
		if (anArray[i]<0) neg++;
		if (anArray[i]%2==0) even++;
		if (anArray[i]%2==1) odd++;
		if(anArray[i]==0) zero++;
		
	}
			System.out.printf("positive: %d, negative: %d, odd: %d, even: %d, zero's: %d ",pos,neg,odd,even,zero);
		
	
	}
}
