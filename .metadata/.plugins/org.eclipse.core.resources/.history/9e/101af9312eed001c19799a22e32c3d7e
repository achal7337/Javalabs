package practice;
import java.util.Arrays;
import java.util.Objects;
import java.util.Scanner;

public class practice {
	
	
private double  a,b;
public practice() {
	this.a=0.0;
	this.b=0.0;
}
public practice(double a,double b) {
	this.a=a;
	this.b=b;
}
public practice(practice other) {
	this.a=other.a;
	this.b=other.b;
}

@Override
public int hashCode() {
	return Objects.hash(a, b);
}
@Override
public boolean equals(Object obj) {
	if (this == obj)
		return true;
	if (obj == null)
		return false;
	if (getClass() != obj.getClass())
		return false;
	practice other = (practice) obj;
	return Double.doubleToLongBits(a) == Double.doubleToLongBits(other.a)
			&& Double.doubleToLongBits(b) == Double.doubleToLongBits(other.b);
}
public double getA() {
	return a;
}
public void setA(double a) {
	this.a = a;
}
public double getB() {
	return b;
}
public void setB(double b) {
	this.b = b;
}
public double sum() {
	return a+b;
}

	
public static void main(String[] args) {
	practice a =  new practice(10.256,5.256);
	System.out.print(a.getA()+" "+ a.b+" " +a.sum()+"/n");

	a.setA(12.36);
	System.out.print(a.getA()+" "+ a.b+" " +a.sum());

	 }
	

}
