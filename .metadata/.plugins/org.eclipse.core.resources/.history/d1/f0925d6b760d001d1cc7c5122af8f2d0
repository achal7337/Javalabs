import java.util.Date;

public class Person {

	private String name;
	private Date date;

	

	public Person(String name, Date date) {
		this.name =  name;
		this.date =date;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getName() {
		return this.name;
	}

	public String getDob() {
		
		return date.toString() ;
	}



}
